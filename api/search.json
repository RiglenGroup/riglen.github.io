---
layout: none
---
[
  {%- for post in site.posts %}
  {
    "type"        : {{ post.type | jsonify }},
    "title"       : {{ post.title | jsonify }},
    "url"         : {{ post.url | relative_url | jsonify }},
    "date"        : {{ post.date | jsonify }},
    "description" : "{{ post.description | markdownify | strip_newlines }}"
  }{% unless forloop.last %},{% endunless %}
  {%- endfor %}
  {%- if site.posts.size > 0 %},{% endif %}
  {%- for post in site.data.categories %}
  {
    "type"        : {{ post.type | append: " category" | jsonify }},
    "title"       : {{ post.title | jsonify }},
    "url"         : {{ "/" | append: post.type | append: "/" | append: post.name | jsonify }},
    "date"        : {{ post.date | jsonify }},
    "description" : "{{ post.description | markdownify | strip_newlines }}"
  }{% unless forloop.last %},{% endunless %}
  {%- endfor %}
  {%- if site.data.categories.size > 0 %},{% endif %}
  {%- for post in site.solutions %}
  {
    "type"        : {{ post.type | jsonify }},
    "title"       : {{ post.title | jsonify }},
    "url"         : {{ post.url | relative_url | jsonify }},
    "date"        : {{ post.date | jsonify }},
    "description" : "{{ post.description | markdownify | strip_newlines }}"
  }{% unless forloop.last %},{% endunless %}
  {%- endfor %}
  {%- if site.solutions.size > 0 %},{% endif %}
  {%- for post in site.services %}
  {
    "type"        : {{ post.type | jsonify }},
    "title"       : {{ post.title | jsonify }},
    "url"         : {{ post.url | relative_url | jsonify }},
    "date"        : {{ post.date | jsonify }},
    "description" : "{{ post.description | markdownify | strip_newlines }}"
  }{% unless forloop.last %},{% endunless %}
  {%- endfor %}
  {%- if site.services.size > 0 %},{% endif %}
  {%- for post in site.trainings %}
  {
    "type"        : {{ post.type | jsonify }},
    "title"       : {{ post.title | jsonify }},
    "url"         : {{ post.url | relative_url | jsonify }},
    "date"        : {{ post.date | jsonify }},
    "description" : "{{ post.description | markdownify | strip_newlines }}"
  }{% unless forloop.last %},{% endunless %}
  {%- endfor %}
]
